<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane fx:id="baseAnchorPane" prefHeight="70.0" prefWidth="408.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="com.example.a1project.controllers.SingleTaskDisplayController">
   <children>
      <HBox fx:id="vBox" prefHeight="56.0" prefWidth="384.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <CheckBox fx:id="taskCheckBox" mnemonicParsing="false" onAction="#taskCompleted" prefHeight="62.0" prefWidth="16.0" />
            <Label fx:id="taskNameLabel" prefHeight="50.0" prefWidth="72.0" text="Name">
               <tooltip>
                  <Tooltip fx:id="descriptionTooltip" text="Description" />
               </tooltip>
            </Label>
            <Label fx:id="dueDateLabel" prefHeight="50.0" prefWidth="63.0" text="due date" />
            <Label fx:id="dueTimeLabel" prefHeight="50.0" prefWidth="56.0" text="due time" />
            <Label fx:id="priorityLabel" prefHeight="50.0" prefWidth="35.0" text="priority" />
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#editTask" prefHeight="62.0" prefWidth="51.0" text="Button" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
